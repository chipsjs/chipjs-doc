        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "_id": {
                        "description": "ID of partner"
                      },
                      "name": {
                        "description": "name of partner"
                      },
                      "heroImageURL": {
                        "description": "URL to partner hero image"
