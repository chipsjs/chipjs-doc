                            "description": ""
                          },
                          "action": {
                            "type": "string",
                            "description": ""
                          },
                          "deviceID": {
                            "type": "string",
                            "description": ""
                          },
                          "deviceType": {
                            "type": "string",
                            "description": ""
                          },
                          "user": {
                            "type": "object",
                            "description": "",
                            "properties": {}
                          },
                          "title": {
                            "type": "string",
                            "description": ""
                          }
                        }
                      }
                    },
                    "nextPage": {
                      "type": "number",
                      "description": ""
                    },
                    "prevPage": {
                      "type": "number",
                      "description": ""
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
