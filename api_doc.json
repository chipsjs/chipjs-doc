{
    "getHouseLog": {
        "method_type": "get",
        "url": "http://127.0.0.1:3030/houses/:houseID/activities",
        "request": {
          "path": {
            "houseID": {
              "type": "string"
            }
          },
          "query": {
            "type": "object",
            "properties": {
              "limit": {
                "description": "log limit num",
                "type": "integer",
                "minimum": 1,
                "maximum": 50
              },
              "clientSerial": {
                "description": "serial of the product",
                "type": "string"
              }
            },
            "required": [
              "limit",
              "clientSerial"
            ]
          }
        },
        "response": {
          "success": {
            "type": "array",
            "properties": {
              "entities": {
                "type": "object"
              },
              "house": {
                "type": "object"
              }
            }
          },
          "failure": {

          }
        }
    },
    "getLockID": {
      "method_type": "get",
      "url": "http://127.0.0.1:3030/locks/:lockID",
      "request": {
        "path": {
          "lockID": {
            "type": "string"
          }
        },
        "query": {
          "type": "object",
          "properties": {
            "clientSerial": {
              "description": "serial of the product",
              "type": "string"
            }
          },
          "required": [
            "clientSerial"
          ]
        }
      }
    },
  "getPinCode": {
    "method_type": "get",
    "url": "${base_url}/locks/:lockID/pin",
    "request": {
      "path": {
        "lockID": {
          "type": "string"
        }
      }
    }
  },
  "otherUserPin": {
    "method_type": "put",
    "url": "${base_url}/locks/users/:lockID/:otherUserID/pin",
    "request": {
      "path": {
        "lockID": {
          "type": "string"
        },
        "otherUserID": {
          "type": "string"
        }
      },
      "body": {
        "slot": {
          "description": "111",
          "type": "integer"
        },
        "action": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "pin": {
          "type": "string"
        },
        "accessType": {
          "type": "string"
        }
      }
    }
  }
}

