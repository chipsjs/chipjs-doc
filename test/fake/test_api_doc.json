{
  "basic": {
    "method_type": "post",
    "url": "{base_url}/test/basic",
    "request": {
      "body": {
        "number_param": {
          "type": "integer",
          "minimum": 0,
          "maximum": 100
        },
        "string_basic_param": {
          "type": "string",
          "minxLength": 2,
          "maxLength": 3
        },
        "object_param": {
          "type": "object"
        },
        "array_param": {
          "type": "array"
        },
        "boolean_param": {
          "type": "boolean"
        },
        "null_param": {
          "type": "null"
        }
      }
    }
  },
  "string_types": {
    "method_type": "post",
    "url": "{base_url}/test/string/types",
    "request": {
      "body": {
        "reg_param": {
          "type": "string",
          "pattern": "^(\\([0-9]{3}\\))?[0-9]{3}-[0-9]{4}$",
          "examples": "(888)555-1212"
        },
        "date_time_param": {
          "type": "string",
          "format": "date-time",
          "examples": "2018-11-13T20:20:39+00:00"
        },
        "email_param": {
          "type": "string",
          "format": "email"
        },
        "ip_param": {
          "type": "string",
          "format": "ipv4"
        }
      }
    },
    "response": {
      "success": {
        "type": "string"
      }
    }
  }
}
